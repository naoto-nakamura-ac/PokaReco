{
  "name": "pokareco",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate-latest": "npx knex migrate:latest --knexfile server/db/knexfile.js",
    "migrate-down": "npx knex migrate:down --knexfile server/db/knexfile.js",
    "seed-data": "npx knex seed:run --knexfile server/db/knexfile.js",
    "db:reset": "npx knex migrate:rollback --all --knexfile server/db/knexfile.js && npm run migrate-latest && npm run seed-data",
    "build": "cd front && npm install && npm run build && rm -rf ../public && cp -r dist ../public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "mocha": "^11.3.0",
    "nodemon": "^3.1.10"
  }
}
